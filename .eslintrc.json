{
	"env": {
		"node": true,
		"browser": true,
		"jquery": true
	},
	"extends": ["airbnb", "eslint:recommended"],
	"rules": {
		// https://eslint.org/docs/rules/
		"no-console": 0,
		"no-undef": 0,
		"no-unused-vars": 0,
		"no-useless-escape": 0,
		"no-extra-boolean-cast": 0,
		"no-extra-semi": 0
	}
}